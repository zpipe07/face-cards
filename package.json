{
  "name": "face-cards",
  "version": "0.0.1",
  "description": "A boilerplate for getting a web application off the ground using React and Babel for JSX.",
  "main": "server.js",
  "scripts": {
    "start": "rimraf ./build && webpack && babel-register server",
    "test": "jest"
  },
  "private": true,
  "dependencies": {
    "babel-jest": "^22.0.3",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-register-cli": "^5.0.0",
    "babili-webpack-plugin": "^0.1.2",
    "copy-webpack-plugin": "^4.3.0",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.0.3",
    "jest-css-modules": "^1.1.0",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "serve-static": "^1.13.1",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/react-babel"
  },
  "engines": {
    "node": "8.1.x"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "react",
      "stage-2",
      "env"
    ]
  },
  "transform": {
    "^.+\\.jsx?$": "babel-jest"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    }
  }
}
